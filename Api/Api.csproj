    <UserSecretsId>4d450ee6-7206-4c1f-9720-8f7d8138623d</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions.CosmosDB" Version="3.0.8" />
